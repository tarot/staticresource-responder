{
    "name": "staticresource-responder",
    "version": "0.0.0",
    "manifest_version": 2,
    "description": "staticresource-responder",
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": "images/icon-128.png"
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "https://*.force.com/*",
      "https://*.salesforce.com/*",
      "storage"
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "options_page": "html/index.html"
}
